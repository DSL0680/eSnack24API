<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.esnack24api.esnack24api.allergy.mapper.AllergyMapper">

    <select id="getKoAllergyList" resultType="KoAllergyListDTO">

        select
            ano, atitle_ko
        from
            tbl_allergy
        where
            ano > 0
    </select>

</mapper>